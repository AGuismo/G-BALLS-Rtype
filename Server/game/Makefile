NAME	     ?=	libgame.a


SRCS =	      	GameManager.cpp		\
		GameException.cpp	\

F.C	     +=	-W -Wall -Werror
F.I	     +=	-I $(DIR.H)
F.D	     +=


all:		start $(NAME)

$(NAME):	lib_compile

clean:		remove_objects

fclean:		clean remove_name

re:		fclean all























































include .libmake
